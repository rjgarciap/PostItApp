<?xml version="1.0" encoding="utf-8" ?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
  <item android:state_pressed="true" >
    <shape>
      <solid
          android:color="#6D8EDB" />
      <stroke
          android:width="1dp"
          android:color="#022B89" />
      <corners
          android:radius="6dp" />
      <padding
          android:left="10dp"
          android:right="14dp"/>
    </shape>
  </item>
  <item>
    <shape>
      <gradient
          android:startColor="#6D8EDB"
          android:endColor="#464759"
          android:angle="270" />
      <stroke
          android:width="1dp"
          android:color="#022B89" />
      <corners
          android:radius="6dp" />
      <padding
          android:left="10dp"
          android:right="10dp" />
    </shape>
  </item>
</selector>